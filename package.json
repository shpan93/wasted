{
  "name": "Universal-React",
  "version": "1.0.0",
  "description": "boilerplate for universal apps",
  "main": "index.js",
  "scripts": {
    "dev": "node webpack-dev-server.js & PORT=3002 NODE_ENV=local nodemon server.js -e js",
    "dev-windows": "node_modules/.bin/npm-run-all --parallel dev-webpack-windows dev-nodemon-windows",
    "dev-webpack-windows": "node webpack-dev-server.js",
    "dev-nodemon-windows": "set PORT=3002&& set NODE_ENV=local&&nodemon server.js -e js",
    "build": "webpack --config webpack.config.prod.js",
    "lint": "eslint ./ --ext .js",
    "lint-fix": "eslint ./ --ext .js --fix",
    "deploy": "gh-pages -d public",
    "relay": "relay-compiler --src ./src --schema path/schema.graphql",
    "start": "NODE_ENV=production node server.js",
    "stylelint": "stylelint \"sass/**/*.scss\" --syntax scss; exit 0"
  },
  "pre-push": [
    "lint",
    "stylelint"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "react-router",
    "redux"
  ],
  "author": "Anton Shpylenia",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "express-basic-auth": "^1.1.1",
    "global": "^4.3.2",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "^4.4.5",
    "react-relay": "^1.0.0",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "stylelint-scss": "^2.1.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.10.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss-loader": "1.3.3",
    "pre-push": "^0.1.1",
    "react-hot-loader": "^1.3.0",
    "relay-compiler": "^1.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "0.16.1",
    "stylelint": "^8.1.0",
    "stylelint-config-standard": "^17.0.0"
  }
}
